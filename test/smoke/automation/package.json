{
  "name": "vscode-automation",
  "version": "0.0.1",
  "description": "VS Code UI automation driver",
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  },
  "license": "MIT",
  "main": "./out/index.js",
  "private": "false",
  "scripts": {
    "postinstall": "npm run compile",
    "compile": "npm run copy-driver && npm run copy-driver-definition && tsc",
    "copy-driver": "mkdirp out && ncp src/driver.js out/driver.js",
    "copy-driver-definition": "node tools/copy-driver-definition.js",
    "copy-package-version": "node tools/copy-package-version.js",
    "prepublishOnly": "TypeScriptESBuild Source/**/*.ts"
  },
  "devDependencies": {
    "@types/debug": "4.1.10",
    "@types/mkdirp": "1.0.2",
    "@types/ncp": "2.0.7",
    "@types/node": "20.x",
    "@types/tmp": "0.2.5",
    "cpx2": "6.0.1",
    "mkdirp": "3.0.1",
    "ncp": "2.0.0",
    "npm-run-all": "4.1.5",
    "tmp": "0.2.1",
    "tree-kill": "1.2.2",
    "typescript": "5.2.2",
    "vscode-uri": "3.0.8",
    "watch": "1.0.2",
    "typescript-esbuild": "0.3.1"
  },
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "type": "module",
  "publishConfig": {
    "access": "public"
  }
}
